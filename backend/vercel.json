{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/voice-upload",
      "dest": "/api/voice-upload.py"
    },
    {
      "src": "/api/today-entries",
      "dest": "/api/today-entries.py"
    },
    {
      "src": "/api/today-totals",
      "dest": "/api/today-totals.py"
    },
    {
      "src": "/api/entries",
      "dest": "/api/entries.py"
    },
    {
      "src": "/api/entries/([a-f0-9\\-]+)",
      "dest": "/api/entry.py"
    },
    {
      "src": "/api/entries/([a-f0-9\\-]+)/items",
      "dest": "/api/entry-items.py"
    },
    {
      "src": "/api/daily-totals",
      "dest": "/api/daily-totals.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/weight-entries",
      "dest": "/api/weight-entries.py"
    },
    {
      "src": "/api/weight-entries/([0-9]+)",
      "dest": "/api/weight-entries.py"
    },
    {
      "src": "/api/user-goals",
      "dest": "/api/user-goals.py"
    },
    {
      "src": "/api/weight-history",
      "dest": "/api/weight-history.py"
    },
    {
      "src": "/api/weight-history/(latest|summary)",
      "dest": "/api/weight-history.py"
    },
    {
      "src": "/api/calorie-history",
      "dest": "/api/calorie-history.py"
    },
    {
      "src": "/api/calorie-history/(summary|today)",
      "dest": "/api/calorie-history.py"
    }
  ]
}